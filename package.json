    "@babel/preset-env": "^7.12.17",
    "babel-loader": "^8.2.2",
    "css-minimizer-webpack-plugin": "^1.2.0",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "mini-css-extract-plugin": "^1.3.8",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "webpack": "^5.23.0",
    "webpack-cli": "^4.5.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.12.1",
    "clean-webpack-plugin": "^3.0.0",
